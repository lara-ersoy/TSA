# PART 2

import pandas as pd
import yfinance as yfin


# (a) 
# Loading in data
df = yfin.download("^FVX", start="2000-02-01", end="2026-01-01", auto_adjust=False, progress=False)[["Adj Close"]]

df.dropna(inplace=True)
df.columns = ["Price"]

# Saving cleaned data
df.to_csv("FVX_data_cleaned.csv")
print(df.head())
print(df.tail())